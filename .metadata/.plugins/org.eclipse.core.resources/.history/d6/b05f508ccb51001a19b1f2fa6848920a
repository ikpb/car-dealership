package com.ikpb.driver;

import com.ikpb.daoimpl.CarImpl;
import com.ikpb.pojo.Car;

public class CarDriver {

	public static void main(String[] args) {
		CarImpl cars = new CarImpl();
		cars.getCarsListInitial();
		cars.addCar();
		cars.getCarsList();
		cars.addCar();
		cars.addCar();
		cars.getCarsList();
		cars.deleteCar();
		cars.getCarsList();
		cars.addCar();
		cars.getCarsList();
		cars.saveCarList();

	}

}
